var flipAndInvertImage = function(image) {
    let n=image.length
    for(let i=0;i<n;i++){
        let m=image[i].length
        for(let j=0;j<m;j++){
            if(image[i][j]!==1){
                image[i][j]=1
            }else{
                a=image[i][j]=0
            }
        }
    }
    for(let i=0;i<n;i++){
      image[i].reverse()
    }
    return image
};
