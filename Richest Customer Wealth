var maximumWealth = function(accounts) {
    let max=0
    for(let i=0;i<accounts.length;i++){
        let sum=0
        let n=accounts[i].length
        for(let j=0;j<n;j++){
            sum+=accounts[i][j]
            if(sum>max){
                max=sum
            }
        }
    }
    return max
};
